<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="content-type" content="text/html; charset=UTF-8">
    <meta charset="utf-8">
    <title>Alinet</title>
    <meta name="generator" content="Bootply" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    {% block stylesheet %}
    <link rel="stylesheet" type="text/css" href="{{ asset('bundles/tramite/templates/css/bootstrap.min.css') }}" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="{{ asset('bundles/tramite/templates/css/styles.css') }}" rel="stylesheet">
    {% endblock stylesheet %}
</head>
<body>
{% block nav %}
<div class="navbar navbar-inverse navbar-fixed-top">
    <div class="container">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            {#<a class="navbar-brand" href="#">Alinet</a>#}
        </div>
        <div class="collapse navbar-collapse">
            <ul class="nav navbar-nav ">
                {% if is_granted('ROLE_HABANA') or is_granted('ROLE_PINAR') or is_granted('ROLE_VILLA_CLARA') or is_granted('ROLE_CAMAGUEY') or is_granted('ROLE_GRANMA') or is_granted('ROLE_SANTIAGO') %}
                <li class="active"><a href="{{ path('usuario') }}">Control de Usuarios</a></li>
                <li><a href="{{ path('documento') }}">Ordenes</a></li>
                {% endif %}
                <li ><a href="{{ path('index') }}">Auntenticarse</a></li>
            </ul>
        </div><!--/.nav-collapse -->
    </div>
</div>
{% endblock nav %}
<div  class="container col-md-12 center-block ">
    {% block  body %}
    <div class="text-center">
        <h1>Bootstrap starter template</h1>
        <p class="lead">Use this document as a way to quickly start any new project.<br> All you get is this text and a mostly barebones HTML document.</p>
    </div>
    {% endblock body %}
</div><!-- /.container -->

<!-- script references -->
{% block  javascript%}
    <script type="text/javascript" src="{{ asset('bundles/tramite/jqgrid-4.6.0/jquery.min.js') }}"></script>
{#<script type="text/javascript" src="{{asset('bundles/tramite/templates/js/jquery.js')  }}"></script>#}
<script type="text/javascript" src="{{asset('bundles/tramite/templates/js/bootstrap.js')  }}"></script>
{% endblock  javascript%}
</body>
</html>