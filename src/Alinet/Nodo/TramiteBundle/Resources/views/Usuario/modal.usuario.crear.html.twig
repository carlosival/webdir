<div id="dialog-form-crear" title="Crear Usuario">
    <div id="users-contain" class="ui-widget">
        <p class="validateTips">All form fields are required.</p>
        <form id="myform" action="{{ path('check') }}" method="post">
            <fieldset>
                <label for="name">Nombre </label>
                <input type="text" placeholder="Usuario" id="_username" name="_username"  class="text ui-widget-content ui-corner-all">
                <label for="email">Correo</label>
                <input type="email" placeholder="Correo" id="_correo" name="_correo"  class="text ui-widget-content ui-corner-all">
                <label for="password">Contrase√±a</label>
                <input type="password"  id="_password" name="_password" placeholder="Contrase&ntilde;a" class="text ui-widget-content ui-corner-all">
                <!-- Allow form submission with keyboard without duplicating the dialog button -->
                <input type="submit" tabindex="-1" style="position:absolute; top:-1000px">
            </fieldset>
        </form>
    </div>
</div>
