{% extends '::base4.html.twig' %}

{% block body %}
<div class="col-md-offset-2 col-md-8">
    <h1>Subir Documento</h1>
    <ul class="record_actions pull-right">
        <li>
            <a href="{{ path('documento') }}">
                Ver Todos
            </a>
        </li>
    </ul>

{{ form_start(form) }}
    {{ form_errors(form) }}

<div class="form-group">
    {{ form_label(form.file) }}
    {{ form_errors(form.file) }}
    {{ form_widget(form.file) }}
</div>

<div class="form-group">
    {{ form_label(form.tramite) }}
    {{ form_errors(form.tramite) }}
    {{ form_widget(form.tramite,{ 'attr': {'class': 'form-control'} }) }}
</div>

    {{ form_widget(form.submit,{ 'attr': {'class':'btn btn-default','label': 'Aceptar','value':'Aceptar'} }) }}

{{ form_end(form) }}

</div>
{% endblock %}
